   "source": [
    "# Function to return text response from client query"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 22,
   "id": "4a04e5b7-dd70-4ec5-81dc-af27d476f347",
   "metadata": {},
   "outputs": [],
   "source": [
    "def answerme(client, question):\n",
    "\n",
    "    resp = client.chat.completions.create(model=\"gpt-3.5-turbo\", messages=[{\"role\": \"user\", \"content\": question}])\n",
    "\n",
    "    return resp.choices[0].message.content"
   ]
  },
